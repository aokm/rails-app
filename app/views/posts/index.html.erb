<h1>Posts#index</h1>

<table>
  <thead>
      <tr>
        <th>title</th>
        <th>description</th>
        <th>body</th>
      </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.description %></td>
        <td><%= post.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>